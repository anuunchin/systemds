# simpleTest1.dml

A = matrix(1, rows=6000, cols=1) %*% matrix(seq(1, 5000, 1), rows=1, cols=5000);

S = matrix(0.5, rows=6000, cols=1);

t1 = time();

Q = floor(A * S);

C = compress(Q);

t2 = time();

dummy = sum(C);
print("Checksum (forces execution):");
print(dummy);

elapsed_time = (t2 - t1) / 1000000;
print("ELAPSED TIME")
print(elapsed_time);
