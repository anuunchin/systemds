# Create a random 6x5 matrix M with values between 0 and 10
M = rand(rows=6, cols=5, min=0, max=10);

# Matrix scaling factor
S = rand(rows=6, cols=5, min=0, max=10);

# Apply quantization: M2 = floor(M * S)
M2 = floor(M * S);

# Print the results
print("Original Matrix M:");
print(toString(M));

print("After applying floor(M * S) where S = " + toString(S) + ":");
print(toString(M2));
